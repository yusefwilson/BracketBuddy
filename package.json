{
  "name": "BracketBuddy",
  "version": "1.0.0",
  "description": "BracketBuddy is the best desktop bracket management app.",
  "exports": "./build-electron/src-electron/main.js",
  "main": "./build-electron/src-electron/main.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"cd src-react && npm run dev\" \"tsc --watch\" \"tsc -p tsconfig.preload.json --watch\" \"wait-on ./build-electron/src-electron/main.js && electronmon --watch ./build-electron/src-electron/main.js\"",
    "build": "npm run build:react && npm run build:ts && npm run build:preload",
    "build:react": "cd src-react && npx vite build",
    "build:ts": "tsc",
    "build:preload": "tsc -p tsconfig.preload.json",
    "package": "electron-forge package",
    "make": "npm run build && electron-forge make"
  },
  "author": "Yusef Wilson",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-wix": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "electron-icon-builder": "^2.0.1",
    "electronmon": "^2.0.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.12",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@types/electron-squirrel-startup": "^1.0.2",
    "flatted": "^3.3.3",
    "tournament-pairings": "^2.0.1",
    "util": "^0.12.5"
  }
}